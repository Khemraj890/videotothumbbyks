{
    "name": "ksolutions/videotothumbbyks",
    "description": "A package to create thumbnails from videos and images for Laravel and Core PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Khemraj",
            "email": "your-email@example.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "KSolutions\\VideoToThumb\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "php-ffmpeg/php-ffmpeg": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KSolutions\\VideoToThumb\\VideoToThumbServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

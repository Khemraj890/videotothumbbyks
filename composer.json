{
    "name": "ksolutions/videotothumbbyks",
    "description": "A package to create thumbnails from videos and images for Laravel and Core PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Khemraj",
            "email": "your-email@example.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "KSolutions\\VideoToThumb\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "php-ffmpeg/php-ffmpeg": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KSolutions\\VideoToThumb\\VideoToThumbServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
